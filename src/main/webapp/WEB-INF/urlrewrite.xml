<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <rule>
        <from>^/index$</from>
        <to>/userController/index.htm</to>
    </rule>
    <rule>
        <from>^/home$</from>
        <to>/userController/main.htm</to>
    </rule>

    <rule>
        <from>^/error/404$</from>
        <to>/errorController/404.htm</to>
    </rule>

    <rule>
        <from>^/error/500$</from>
        <to>/errorController/500.htm</to>
    </rule>
</urlrewrite>